
<%= form_tag("/album_codes/create", class: "ajax_update") do %>


  <%= label_tag(:times, "Number of Codes") %>
  <%= number_field_tag(:to_create) %>
  <%= hidden_field_tag 'id', @album.id %>
  <%= hidden_field_tag 'url_slug_hidden', params[:url_slug] %>
  <%= submit_tag("Create", class:"btn")  %>


<% end %>

