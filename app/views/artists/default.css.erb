    <% if @artist.profile_layout.use_css == true %>

    <%= @artist.profile_layout.profile_css %>

    <% else %>
        <% @artist.profile_layout.h2_colour = @artist.profile_layout.h2_colour.blank? ? "#999" : @artist.profile_layout.h2_colour %>

    #content p {
        font-family: <%= @artist.profile_layout.content_font %>;
        font-size: <%= @artist.profile_layout.p_size %>pt;
        color: <%= @artist.profile_layout.p_colour %>;
    }

    #logo h1 {
        font-family: <%= @artist.profile_layout.logo_font %>;
        font-size: <%= @artist.profile_layout.h1_size %>px;
        color: <%= @artist.profile_layout.h1_colour %>;
        text-align: center;
    }

    h1 {
        font-family: <%= @artist.profile_layout.content_font %>;
        font-size: <%= @artist.profile_layout.h1_size %>px;
        color: <%= @artist.profile_layout.h1_colour %>;
        text-align: center;
    }

    h2, .buy_header  {
        font-family: <%= @artist.profile_layout.content_font %>;
        font-size: <%= @artist.profile_layout.h2_size %>px;
        color: <%= @artist.profile_layout.h2_colour %>;
        text-align: center;
    }

    h3 {
        font-family: <%= @artist.profile_layout.content_font %>;
        font-size: <%= @artist.profile_layout.h3_size %>px;
        color: <%= @artist.profile_layout.h3_colour %>;
        text-align: center;
    }

