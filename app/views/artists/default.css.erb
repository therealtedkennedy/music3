@-moz-document url-prefix() {

    .imageHover {
        display: block;
        position: relative;
    }

    /*firefox fix for album index*/
    .album .imageHover, .song-table .imageHover {
        display: inline-block;
        position: relative;
    }


    object:active, div:active { outline: none; }

    object:focus, div:focus { outline: none; }

}


.imageHover {
    display: inline-block;
    position: relative;
}



.hoverPlay span {
    display: none;
}

.non-mobile .imageHover:hover .hoverPlay {
    position: absolute;
    top: 0;
    bottom: 0;
    right: 0;
    left: 0;
    background: rgba(0,0,0,.3);
}

.non-mobile .imageHover:hover .hoverPlay span {
    display: block;
    position: absolute;
    top:50%;
    left:50%;
    text-align: center;
    margin-top: -15px;
    margin-left: -15px;

    background: url(../../images/sprites.png) -192px 0px no-repeat;

    width: 30px !important;
    height: 30px;
}

.jp-playlist .imageHover:hover .hoverPlay {
    display: none;
}

.jp-playlist .imageHover:hover .hoverPlay span {
    margin-top: -32px;
}

.songList li {
    position: relative;
}

.songList li:hover ::after .hoverPlay span{
    background: url(../../images/sprites.png) -192px 0px no-repeat;
    display: block;
    position: absolute;
    top:50%;
    left:50%;
    text-align: center;
    margin-top: -32px;
    margin-left: -15px;

    width: 30px !important;
    height: 30px;
}

.social-media-icons {
    font-size: 32px;
}

.social-media-icons a:hover {
    text-decoration:none;
    cursor:pointer;
    color: <%= @artist.profile_layout.h2_colour %>;
}