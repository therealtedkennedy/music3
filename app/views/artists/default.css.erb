/** {margin:0;padding:0}*/

    html,body
    {
        margin:0;
        padding:0;
        /*overflow:hidden;  removed by Ted.  Admin page needs scroll */
    }

    body
    {
        height:100%;
    }

    @font-face {
        font-family: 'icomoon';
        src: url("/assets/fonts/iconmoon.eot");
    }


    .container {
        position:absolute;
        left:0;
        right:0;
        top:0;
        height:50px;
        overflow:hidden;
    }

    .bodyArea
    {
        overflow:auto;

        margin-top 50px;
        padding:70px;

        /*background-color:#f4e2d2;*/
    }

    .bodyArea .floatingContent
    {
        height:100%;
        display:block;
        margin: 0 auto;
        /*position: relative;  removed by ted. */
    }

    .bodyArea .floatingContent
    {
        text-decoration: none;
    }



    .bodyArea .artistLogoArea
    {
        margin: 0 auto;
        width: 200px;
        height:150px;
        text-align: center;
        display:block;
    }

    .artistLogoArea img {
        max-width: 200px;
        max-height: 150px;
    }

    .bodyArea .navArea
    {
        margin-top: 50px;
		margin-right: auto;
		margin-left:auto;

        text-align: center;
        display:block;
    }

        .bodyArea .nav
        {
            width:25%;
            text-align: center;
            cursor: pointer;
            height:50px;
            display:inline-block;
            font-family: 'verb_regularbold', sans-serif;
            font-size:20px;
            color:#96938f;
            /*text-shadow: 0px 0px 1px rgba(0,0,0,0.4); */
        }

        .bodyArea .nav:hover
        {
            color:#5b5956;
        }

        .bodyArea .nav.selected
        {
            font-family: 'verb_regularbold', sans-serif;
            color:#000;
            background-image: url("/images/indicator.png");
            background-position: bottom center;
            background-repeat: no-repeat;
        }

    .bodyArea .miniPage
    {
        min-height:300px;
		max-height: 350px;
        /*max-height:600px;*/

        
        background-image: url('/images/70black.png');
        overflow:auto;
        margin:0 auto;
        bottom:0;
        padding:30px;
        font-family: 'verb_regularregular';
        color:#fff;
    }

	.bodyArea .editPage
	{
		min-height:300px;

		/*max-height:600px;*/


		background-image: url('/images/70black.png');
		overflow:auto;
		margin:0 auto;
		bottom:0;
		padding:20px;
		font-family: 'verb_regularregular';
		color:#fff;
	}

    .miniPage .album
    {
        width:290px;
        height:300px;
        text-align: center;
        cursor: pointer;
        display: inline-block;
        vertical-align: top;
    }

    .miniPage .album:hover
    {
        background-image: url('/images/10black.png');
    }

    .miniPage .album img
    {
        width:200px;
        height:200px;
        margin:0 auto;
        display:block;
        border:0;
    }

    .miniPage .album h1
    {
        font-family: 'verb_regularbold';
        font-size: 18px;
        margin:0 auto;
        display:block;
        padding:0px;
        margin:0;
        margin-top:10px;
        color:#fff;
    }

    .miniPage .album h2
    {
        font-family: 'verb_lightregular';
        color:#aaa;
        font-size: 14px;
        margin:0 auto;
        display:block;
        padding:0px;
        margin:0;
        margin-top:5px;
    }

    .vat {
        vertical-align:top;
    }

    .w100 {
        width: 100%;
    }



        /* Defines whole top nav, including sign in/out, and the player it self */
    .player_nav {
        width: 100%;

        height: 50px;
        position: fixed;
        top: 0;
        left: 0;
        background: #2E2E2E;

    }

        /* Sign in/sign out */
    .user_nav {
        position: fixed;
        height: 50px;
        line-height: 50px;
        padding-right: 15px;
        color: #C4C4C4;
        right: 0;
        top: 0;
    }

    .user_nav_info {
        display: inline-block;

    }

    .admin {
        display: inline-block;
        border-left: 1px solid #ffffff;
        line-height: 30px;
        padding: 5px;

    }

    .adminLink {
        color: #ffffff;
    }

    .audio {
        position: fixed;
        top: 0;
        margin: 0 auto;
        background-color: #FFFFFF;
        width: 9999px;
        height: 50px
    }

    a.nav, .adminLink {
        color: #FFFFFF;
        text-decoration: none;

    }

    a.nav:hover, .adminLink:hover {
        color: #FFFFFF;

        font-weight: bold;
        text-decoration: none;
    }


    #text_area {
        width: 350px;
        max-height: 200px;
        overflow: auto;
        margin-bottom: 15px;
    }


	.edit-controls {
		display:none;
		float: right;
		/*display: inline-block;*/
		width: 320px;
		background: #2E2E2E;
		padding: 5px;
        margin-top: 50px;
	}

    /*info*/
    .left-panel {
      padding:15px 30px 15px 30px;
    }

    .left-panel img {
        border: 1px solid #666;
    }

    .info-image {
      margin-bottom:10px;
      max-width: 400px;
      max-height:400px;
    }

    .info-container {
      font-size: 16px;
      vertical-align: top;
      padding: 0px 0px 0px 30px;
    }

    .info-container table {
      width: 95%;
    }

    .row-border tr {
      border-top: 1px solid #333;
    }

    .info-item {
      padding-left:30px; 
      width:35%;
      vertical-align: top;
    }

    .item-description {
      padding-left:30px; 
      color:#999;
    }

    .bio {
      color:#999;
    }

    /*songs*/
    #song-content {
        min-height: 300px;
    }

    table.song-table {
        width:100%; 
        height:100%;
    }

    table.song-table thead {
        border-bottom: 2px solid #333;
    }

    table.song-table tbody {
        color:#999; 
        font-size:90%;
    }

    a.content-link {
        color: #999;
        text-decoration: none;
    }

    a.music-button {
        text-decoration: none;
    }

    div.song-footer {
        text-align: center; 
        font-size: 90%; 
        color: #999;
    }

    /*chrome custom scrollbar*/
    ::-webkit-scrollbar {
        width: 12px;
    }
     
    ::-webkit-scrollbar-track {
        -webkit-box-shadow: inset 0 0 20px rgba(200,200,200,0.3); 
        border-radius: 10px;
    }
     
    ::-webkit-scrollbar-thumb {
        border-radius: 10px;
        -webkit-box-shadow: inset 0 0 20px rgba(200,200,200,0.5); 
    }

    #promo {
        text-align: center;
        position:absolute;
        z-index: 999;
        width: 90%;
        background-color: #2e2e2e;
        margin-left: -35px;
        margin-top: -35px;
        border: 1px solid #000;
    }

    #promo img {
        padding: 30px;
    }



