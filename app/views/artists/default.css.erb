        font-family: <%= @artist.profile_layout.content_font %>;
        font-size: <%= @artist.profile_layout.p_size %>pt;
    }

    ul {
        font-family: <%= @artist.profile_layout.content_font %>;
        font-size: <%= @artist.profile_layout.p_size %>pt;
    }

